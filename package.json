{
	"name": "portfolio-website-react",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/node": "^18.15.5",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1",
		"typescript": "^5.0.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"build:gh-pages": "set PUBLIC_URL=https://riyadh-dev.github.io/portfolio-website-react/&& react-scripts build",
